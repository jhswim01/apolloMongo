  "dependencies": {
    "apollo-server": "^2.16.1",
    "apollo-server-express": "^2.16.1",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "graphql": "^15.3.0",
    "graphql-playground-middleware-express": "^1.7.18",
    "graphql-tools": "^6.0.16",
    "helmet": "^4.0.0",
